#!/usr/bin/env bash

python train.py \
  --train_data lmdb_train \
  --valid_data lmdb_val \
  --saved_model my_ctc_finetune_starter.pth \
  --select_data "/" \
  --batch_ratio 1.0 \
  --Transformation None \
  --FeatureExtraction VGG \
  --SequenceModeling BiLSTM \
  --Prediction CTC \
  --workers 4 \
  --batch_size 256 \
  --num_iter 20000 \
  --valInterval 100 \
  --adam --sensitive \
  --lr 0.0001 \
  --character "()+123456ABCFHKMNOPSZaeglnu·↑→↓光加温点热照燃电通高"